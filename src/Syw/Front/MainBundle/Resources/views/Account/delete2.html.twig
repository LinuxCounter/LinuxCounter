{% extends "SywFrontMainBundle::base.html.twig" %}
{% trans_default_domain "syw_front_main_account_delete" %}

{% block body %}

    <!-- Page Content -->
    <div class="row">
        <div class="row">
            <div class="col-sm-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">{{ 'Deletion in process...'|trans }}</h3>
                    </div>
                    <div class="panel-body">

                        {% for label, flashes in app.session.flashbag.all %}
                            {% for flash in flashes %}
                                <div class="alert alert-{{ label }}">
                                    {{ flash }}
                                </div>
                            {% endfor %}
                        {% endfor %}





                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
